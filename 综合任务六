<script>
	var _main = document.getElementById("main");
	var _button1 = document.getElementById("button1");
	var _button2 = document.getElementById("button2");
	//网页弹窗功能计时          
	window.onload = function(){
		setTimeout('_main.style.display = "block"',1000);
	}
	_button1.onclick = function(){
		_main.style.display = "none";
		alert("正在打开联系客户");
	}
	_button2.onclick = function(){
		_main.style.display = "none";
	}
</script>
</body>
